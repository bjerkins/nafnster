{
  "rules": {
    "names": {
      "male": {
        ".read": "auth != null",
        ".write": "auth != null",
        ".indexOn": [
          "name"
        ]
      }
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}
